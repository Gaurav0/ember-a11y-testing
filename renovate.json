{
  "extends": [
    "config:base",
    "schedule:nonOfficeHours"
  ],
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "packagePatterns": [ "eslint" ],
      "groupName": "eslint"
    },
    {
      "packagePatterns": [ "sass" ],
      "groupName": "sass"
    },
    {
      "packageNames": [
        "broccoli-funnel",
        "ember-cli",
        "ember-cli-babel",
        "ember-cli-dependency-checker",
        "ember-cli-htmlbars",
        "ember-cli-htmlbars-inline-precompile",
        "ember-cli-inject-live-reload",
        "ember-cli-uglify",
        "ember-export-application-global",
        "ember-source",
        "ember-resolver"
      ],
      "groupName": "ember packages"
    },
    {
      "packageNames": [
        "qunit-dom",
        "ember-try",
        "ember-source-channel-url",
        "ember-sinon"
       ],
      "groupName": "ember tests packages"
    }
  ]
}
